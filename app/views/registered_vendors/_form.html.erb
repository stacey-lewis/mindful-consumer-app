<%= form_with model: @registered_vendor, local: true do |f| %>

<%= f.label :vendor_name %>
<%= f.text_field :vendor_name %> <br>
<br>

<%= f.label :phone_number %>
<%= f.text_field :phone_number %> <br>
<br>

<%= f.label :address %>
<%= f.text_field :address %> <br>
<br>

<%= f.label :suburb %>
<%= f.text_field :suburb %> <br>
<br>

<%= f.label :state %>
<%= f.text_field :state %> <br>
<br>

<%= f.label :postcode %>
<%= f.text_field :postcode %> <br>
<br>

<%= f.label :website %>
<%= f.text_field :website %> <br>
<br>

<%= f.label :logo, "Logo image URL" %>
<%= f.text_field :logo %> <br>
<br>

<%= f.label :background_image, "Background image URL" %>
<%= f.text_field :background_image %> <br>
<br>

<%= f.label :hero_image, "Hero image URL" %>
<%= f.text_field :hero_image %> <br>
<br>

<%= f.label :opening_hours %>
<%= f.text_field :opening_hours %> <br>
<br>

<%= f.label :bio, "Short bio about store" %>
<%= f.text_field :bio %> <br>
<br>

<%= f.submit%>

<% end %>
